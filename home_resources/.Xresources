!Xft.dpi: 300

!Xcursor.size:12

!General Things
URxvt.tabbed.no-tabbedex-keys: true
urxvt*shell:/usr/bin/zsh
urxvt*loginShell:true
!urxvt*inheritPixmap: true
urxvt*tintColor: #5e2750
urxvt*transparent:true
urxvt*shading: 100
urxvt*fading: 30
urxvt*scrollBar:false
!urxvt*transpscrollbar:true
urxvt*saveLines:50000


! Font
!URxvt*letterSpace: 4
URxvt*font:xft:Bitstream Vera Sans Mono:autohint=true:pixelsize=10,\
	-misc-fixed-bold-r-normal--15-140-75-75-c-90-iso10646-1,\
	-misc-fixed-iso10646-1,\
	-misc-fixed-medium-r-normal--15-140-75-75-c-90-iso10646-1, \
	xft:Ubuntu Mono:size=11,xft:Monospace:size=9:antialias=true:hinting=true,\
	-misc-fixed-medium-r-normal--18-120-100-100-c-90-iso10646-1,\
	-misc-fixed-medium-r-normal-ja-18-120-100-100-c-180-iso10646-1,\
	xft:Droid Sans Japanese:style=Regular,\
	-Misc-Fixed-Medium-R-Normal--7-70-75-75-C-50-ISO10646-1,\
	-Misc-Fixed-Medium-R-Normal--8-80-75-75-C-50-ISO10646-1,\
	-Misc-Fixed-Medium-R-Normal--9-90-75-75-C-60-ISO10646-1,\
	-Misc-Fixed-Medium-R-Normal--10-100-75-75-C-60-ISO10646-1,\
	-Misc-Fixed-Medium-R-Semicondensed--12-110-75-75-C-60-ISO10646-1,\
	-Misc-Fixed-Medium-R-SemiCondensed--13-120-75-75-C-60-ISO10646-1,\
	-Misc-Fixed-Bold-R-SemiCondensed--13-120-75-75-C-60-ISO10646-1,\
	-Misc-Fixed-Medium-R-Normal--13-120-75-75-C-70-ISO10646-1,\
	-Misc-Fixed-Bold-R-Normal--13-120-75-75-C-70-ISO10646-1,\
	-Misc-Fixed-Medium-R-Normal--14-130-75-75-C-70-ISO10646-1,\
	-Misc-Fixed-Bold-R-Normal--14-130-75-75-C-70-ISO10646-1,\
	-Misc-Fixed-Medium-R-Normal--13-120-75-75-C-80-ISO10646-1,\
	-Misc-Fixed-Bold-R-Normal--13-120-75-75-C-80-ISO10646-1,\
	-Misc-Fixed-Medium-R-Normal--15-140-75-75-C-90-ISO10646-1,\
	-Misc-Fixed-Bold-R-Normal--15-140-75-75-C-90-ISO10646-1,\
	-Misc-Fixed-Medium-R-Normal--20-200-75-75-C-100-ISO10646-1,\
	[codeset=JISX0208]xft:Kochi Gothic:antialias=false, \
	DejaVuSansMono.ttf: "DejaVu Sans Mono" "Book",\
	xft:Uming:pixelsize=28,\
	xft:Code2000:antialias=false,\
	xft:Inconsolata-dz for Powerline:pixelsize=22,\
	xft:Symbola:pixelsize=20

!Icon for file
!URxvt*iconFile: /home/steven/Dropbox/Wallpapers/tumblr_mg0th7CodL1qmxcsxo1_500.gif
!Mouse
urxvt*mouseWheelScrollPage: true

!true transparency
!URxvt.depth: 32
!URxvt.background: rgba:0000/0000/0000/cccc


!starting Dir

!Extendos
URxvt.perl-ext-common: default,matcher,tabbedex,clipboard

!Clipboard
urxvt*keysym.Control-Shift-V: perl:clipboard:paste

!URL Clicker
URxvt.urlLauncher: google-chrome
URxvt.matcher.button: 1
URxvt.matcher.pattern.1: \bwww\.[\w-]\.[\w./?&@#-]*[\w/-]
URxvt.colorUL: #4682B4

!Tabs
URxvt*tabbed.autohide: true
URxvt*tabbed.reopen-on-close: no
URxvt.keysym.Control-t: perl:tabbedex:new_tab
URxvt.keysym.Control-Shit-M: perl:tabbedex:next_tab
URxvt.keysym.Control-Shift-N: perl:tabbedex:prev_tab
!URxvt.keysym.Control-Shift-Left: perl:tabbedex:move_tab_left
!URxvt.keysym.Control-Shift-Right: perl:tabbedex:move_tab_right
!URxvt.keysym.Control-Shift-R: perl:tabbedex:rename_tab

